
{
    "manifest_version": 3,
    "name": "Peloton Output Tracker",
    "version": "1.0",
    "description": "Track your Peloton cycling output progress over time",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [{
      "resources": ["lib/papaparse.min.js", "lib/chart.js"],
      "matches": ["<all_urls>"]
    }]
  }